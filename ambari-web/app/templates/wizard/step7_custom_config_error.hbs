<h5>{{view.message}}</h5>
<br />
<div class="pre-scrollable" style="max-height: 250px;">
  <ul>
    {{#each val in view.customConfig}}
      {{#if val.siteProperties}}
        <li>
          {{val.serviceName}}
            <ul>
              {{#each item in val.siteProperties}}
                <li>
                  {{item.displayMsg}}
                </li>
              {{/each}}
            </ul>
        </li>
      {{/if}}
    {{/each}}
  </ul>
</div>