<table class="table table-bordered table-striped">
  <thead>
  <tr>
    <th>{{t common.service}}</th>
    <th>{{t common.property}}</th>
    <th>{{t installer.step7.popup.currentValue}}</th>
    <th>{{t installer.step7.popup.adjustedValue}}</th>
  </tr>
  </thead>
  <tbody>
  {{#each property in view.propertyChange}}
    <tr>
      <td>{{property.serviceName}}</td>
      <td>{{property.propertyDisplayName}}</td>
      <td>{{property.curValue}}</td>
      <td>{{property.newValue}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
