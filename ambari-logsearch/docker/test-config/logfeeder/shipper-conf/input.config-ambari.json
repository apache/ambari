              "post_value":"0"
            }
          },
          {
            "map_fieldvalue":{
              "pre_value":"TIMEDOUT",
              "post_value":"0"
            }
          },
          {
            "map_fieldvalue":{
              "pre_value":"ABORTED",
              "post_value":"0"
            }
          },
          {
            "map_fieldvalue":{
              "pre_value":"SKIPPED_FAILED",
              "post_value":"0"
            }
          },
          {
            "map_fieldname":{
              "new_fieldname":"result"
            }
          }
        ],
        "ResultStatus":[
          {
            "map_fieldcopy":{
              "copy_name": "ws_result_status"
            }
          },
          {
            "map_fieldvalue":{
              "pre_value":"200 OK",
              "post_value":"1"
            }
          },
          {
