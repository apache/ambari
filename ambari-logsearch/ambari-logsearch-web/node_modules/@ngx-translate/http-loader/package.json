  "author": "Olivier Combe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngx-translate/http-loader/issues"
  },
  "homepage": "https://github.com/ngx-translate/http-loader#readme",
  "main": "bundles/http-loader.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
