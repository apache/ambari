    "reflect-metadata": "0.1.9",
    "rimraf": "2.5.4",
    "rxjs": "5.1.1",
    "semantic-release": "6.3.5",
    "source-map-loader": "0.1.6",
    "ts-helpers": "1.1.2",
    "tslint": "4.4.2",
    "tslint-loader": "3.4.2",
    "typescript": "2.0.10",
    "webpack": "2.2.1",
    "zone.js": "0.7.7"
