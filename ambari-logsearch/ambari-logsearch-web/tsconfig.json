{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@envs/*": [
        "environments/*"
      ],
      "@vendor/*": [
        "vendor/*"
      ],
      "@modules/*": [
        "app/modules/*"
      ],
      "@mockdata/*": [
        "mockdata/*"
      ]
    },
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  }
}
