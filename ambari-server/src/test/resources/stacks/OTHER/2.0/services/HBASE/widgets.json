{
  "layouts": [
    {
      "layout_name": "default_hbase_layout",
      "section_name": "HBASE_SUMMARY",
      "widgetLayoutInfo": [
        {
          "widget_name": "RS_READS_WRITES",
          "description": "This widget shows all the read requests and write requests on all regions for a RegionServer",
          "widget_type": "GRAPH",
          "is_visible": true,
          "metrics": [
            {
              "name": "regionserver.Server.Get_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Get_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER",
              "host_component_criteria": "isActive=true"
            },
            {
              "name": "regionserver.Server.Scan_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Scan_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            },
            {
              "name": "regionserver.Server.Append_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Append_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            },
            {
              "name": "regionserver.Server.Delete_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Delete_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            },
            {
              "name": "regionserver.Server.Increment_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Increment_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            },
            {
              "name": "regionserver.Server.Mutate_num_ops._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/Mutate_num_ops._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            }
          ],
          "values": [
            {
              "name": "Read Requests",
              "value": "`${regionserver.Server.Get_num_ops._rate + regionserver.Server.Scan_num_ops._rate}`"
            },
            {
              "name": "Write Requests",
              "value": "`${metrics.hbase.regionserver.Server.Append_num_ops._rate + metrics.hbase.regionserver.Server.Delete_num_ops._rate + metrics.hbase.regionserver.Server.Increment_num_ops._rate + metrics.hbase.regionserver.Server.Mutate_num_ops._rate}`"
            }
          ],
          "properties": {
            "display_unit": "Requests",
            "graph_type": "LINE",
            "time_ranger": "1 week"
          }
        },
        {
          "widget_name": "OPEN_CONNECTIONS",
          "description": "This widget shows number of current open connections",
          "widget_type": "GRAPH",
          "is_visible": true,
          "metrics": [
            {
              "name": "ipc.IPC.numOpenConnections",
              "ambari_id": "metrics/hbase/ipc/IPC/numOpenConnections",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            }
          ],
          "values": [
            {
              "name": "Open Connections",
              "value": "`${ipc.IPC.numOpenConnections}`"
            }
          ],
          "properties": {
            "display_unit": "Connections",
            "graph_type": "STACK",
            "time_ranger": "1 hour"
          }
        },
        {
          "widget_name": "ACTIVE_HANDLER",
          "default_section_name": "HBASE_SUMMARY",
          "widget_type": "GRAPH",
          "is_visible": false,
          "metrics": [
            {
              "name": "ipc.IPC.numOpenConnections",
              "ambari_id": "metrics/hbase/ipc/IPC/numOpenConnections",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            },
            {
              "name": "ipc.IPC.numCallsInGeneralQueue",
              "ambari_id": "metrics/hbase/ipc/IPC/numOpenConnections",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            }
          ],
          "values": [
            {
              "name": "Active Handlers",
              "value": "`${ipc.IPC.numActiveHandler}`"
            },
            {
              "name": "Calls in General Queue",
              "value": "`${ipc.IPC.numCallsInGeneralQueue}`"
            }
          ],
          "properties": {
            "graph_type": "LINE",
            "time_ranger": "1 hour"
          }
        },
        {
          "widget_name": "FILES_LOCAL",
          "default_section_name": "HBASE_SUMMARY",
          "description": "This widget shows percentage of files local.",
          "widget_type": "NUMBER",
          "is_visible": true,
          "metrics": [
            {
              "name": "regionserver.Server.percentFilesLocal",
              "ambari_id": "metrics/hbase/regionserver/percentFilesLocal",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            }
          ],
          "values": [
            {
              "name": "Files Local",
              "value": "`${regionserver.Server.percentFilesLocal}`"
            }
          ],
          "properties": {
            "display_unit": "%"
          }
        },
        {
          "widget_name": "UPDATED_BLOCKED_TIME",
          "default_section_name": "HBASE_SUMMARY",
          "description": "",
          "widget_type": "GRAPH",
          "is_visible": true,
          "metrics": [
            {
              "name": "regionserver.Server.updatesBlockedTime._rate",
              "ambari_id": "metrics/hbase/regionserver/Server/updatesBlockedTime._rate",
              "category": "",
              "service_name": "HBASE",
              "component_name": "HBASE_REGIONSERVER"
            }
          ],
          "values": [
            {
              "name": "Updated Blocked Time",
              "value": "`${regionserver.Server.updatesBlockedTime._rate}`"
            }
          ],
          "properties": {
            "display_unit": "seconds",
            "graph_type": "LINE",
            "time_ranger": "1 day"
          }
        }
      ]
    }
  ]
}
